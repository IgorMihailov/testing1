all: goldbach                                                        
                                                                     
goldbach: primes.o goldbach.o                                        
        gcc goldbach.o primes.o -o goldbach                          
                                                                     
goldbach.o: goldbach.c primes.h                                      
        gcc -g -O0 -c goldbach.c                                     
                                                                     
primes.o: primes.c primes.h                                          
        gcc -g -O0 -c primes.c                                       
                                                                     
clean:                                                               
        rm all *.o